{
  "webroot": "wwwroot",
  "version": "1.0.0-*",

  "dependencies": {
    "Kestrel": "1.0.0-beta4",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-beta4",
    "Thinktecture.IdentityServer3": "1.5.0",
    "Microsoft.AspNet.Owin": "1.0.0-beta4",
    "Microsoft.Owin": "3.0.1",
    "Microsoft.AspNet.DataProtection": "1.0.0-beta4",
    "Microsoft.AspNet.Diagnostics": "1.0.0-beta4",
    "Autofac": "3.5.2",
    "Microsoft.AspNet.WebApi.Core": "5.2.3",
    "System.IdentityModel.Tokens.Jwt": "4.0.1",
    "Microsoft.AspNet.WebApi.Owin": "5.2.3",
    "Microsoft.Owin.Security": "3.0.1",
    "Autofac.WebApi2": "3.4.0",
    "Microsoft.Owin.Cors": "3.0.1",
    "Microsoft.Owin.Security.Cookies": "3.0.1",
    "Microsoft.Owin.StaticFiles": "3.0.1",
    "Microsoft.AspNet.WebApi.Tracing": "5.2.3",
    "Thinktecture.IdentityModel.Core": "1.4.0",
    "Npgsql": "2.2.3",
    "Dapper": "1.24",
    "Microsoft.Framework.ConfigurationModel.Json": "1.0.0-beta4"
  },

  "commands": {
      "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5000",
      "kestrel": "Microsoft.AspNet.Hosting --server Kestrel --server.urls http://localhost:44319"
  },

  "frameworks": {
    "dnx451": { 
      "frameworkAssemblies": {
        "System.Data": ""
      }
    }
  },

  "publishExclude": [
    "**.xproj"
  ],
  "exclude": [
    "wwwroot"
  ]
}

